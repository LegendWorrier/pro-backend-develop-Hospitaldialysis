{
  "AllowedHosts": "*",
  "Authentication": {
    "Issuer": "http://localhost/"
  },
  "Reports": {
    "Hemosheet": "Hemosheet.trdp",
    "Hemosheet:HasReassessment": true,
    //"Hemosheet:FixedLines": 8,
    //"Hemosheet:FixedLines:Nurse": 0,
    //"Hemosheet:FixedLines:Med": 0,
    //"Hemosheet:FixedLines:Doctor": 0
    //"Hemosheet:NurseInShift": true,
    "HemoRecords": "HemoRecords.trdp",

    "CacheStrategy": "InMemory"
  },
  "GlobalSettings": {
    "LogoAlign": 0
  },
  "ReportMapping": {
    "Coverage": {
      "Cash": "Cash",
      "Gov": "Government",
      "Social": "Social Security",
      "Nation": "National Health Security",
      "Other": "Other"
    }
  },
  "telerikReporting": {
    "fontLibrary": {
      "useDefaultLocations": "true",
      "fontLocations": [
        {
          "path": "/usr/share/fonts",
          "searchSubfolders": "true"
        }
      ]
    }
  }
}
